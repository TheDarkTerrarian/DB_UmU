@model IEnumerable<ParcialTienditaUwU.Models.Sell>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SellDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalToPay)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SellDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalToPay)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SellId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SellId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SellId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
